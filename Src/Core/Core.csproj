<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net35;net462;netstandard2</TargetFrameworks>
    <Product>NEbml Core</Product>
    <Description>Lightweight EBML binary format reader/writer</Description>
    <AssemblyVersion>0.1.4.1</AssemblyVersion>
    <FileVersion>0.1.4.1</FileVersion>
    <PackageId>NEbml</PackageId>
    <Version>0.1.4.1</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>OlegZee</Authors>
    <Company>OlegZee</Company>
    <PackageProjectUrl>https://github.com/OlegZee/NEbml</PackageProjectUrl>
    <PackageReleaseNotes>Added support for NetStandard 2.0</PackageReleaseNotes>
    <Copyright>Copyright 2014-2018</Copyright>
    <PackageTags>EBML MKV Matroska</PackageTags>
    <AssemblyName>NEbml.Core</AssemblyName>
    <RootNamespace>NEbml.Core</RootNamespace>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|net35|AnyCPU'">
    <DefineConstants>TRACE;DEBUG;NET35</DefineConstants>
  </PropertyGroup>

</Project>